#include <iostream>
using namespace std;
#include<algorithm>
void productArray(int *arr,int n)
{
    int left[n]={};
    int right[n]={};
    std::fill_n(left,n,1);
    std::fill_n(right,n,1);
    left[0]=1;
    right[0]=1;
    for(int i=1;i<n;i++){
        left[i]=left[i-1]*arr[i-1];
    }
    for(int i=n-2;i>=0;i--){
        right[i]=arr[i+1]*right[i+1];
    }
    
    for(int i=0;i<n;i++){
        arr[i]=left[i]*right[i];
    }
}

int main()
{
    int t;
    cin>>t;
    while(t>0)
	//code
	{
	    int n;
	    cin>>n;
	    int arr[n];
	   for(int i=0;i<n;i++)
	   {
	       cin>>arr[i];
	   }
	   
	   productArray(arr,n);
	   for(int i=0;i<n;i++){
	       cout<<arr[i]<<" ";
	   }
	   cout<<endl;
	   t--;
	}
	return 0;
}
